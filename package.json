{
  "name": "terminal-automate",
  "displayName": "Terminal Automate",
  "description": "This extension is for add terminals automatic and run commands with an shortcut",
  "repository": "https://github.com/victorcesae/terminal-automate",
  "publisher": "victorcesae",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.reload"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "properties": {
        "terminal-automate.commands": {
          "type": "array",
          "description": "Configuração dos comandos do Terminal Automate"
        }
      }
    },
    "commands": [
      {
        "command": "terminal-automate.waitForKeyPress",
        "title": "Wait for Key Press"
      },
      {
        "command": "terminal-automate.dynamicCommand",
        "title": "Terminal Automate: Execute the dynamic commands"
      },
      {
        "command": "terminal-automate.openForm",
        "title": "Terminal Automate: Open the form for creating the command sequences"
      }
    ],
    "keybindings": [
      {
        "command": "terminal-automate.dynamicCommand",
        "key": "Ctrl+Alt+A",
        "mac": "Cmd+Alt+A",
        "when": "editorTextFocus"
      },
      {
        "command": "terminal-automate.openForm",
        "key": "Ctrl+Alt+R",
        "mac": "Cmd+Alt+R",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.79.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
